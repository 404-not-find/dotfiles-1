activity '~Activity in window %'
altscreen off
autodetach on
backtick 1 86400 86400 whoami
bell '~Bell in window %'
bind b focus bottom
bind j focus down
bind k focus up
bind t focus top
caption always "%{= dd}%{+ m}%1`@%H%{-}: %{+ g}%+20=%-Lw%50>%{=b m}%n%f %t%{= g}%+Lw%-012= %{+ m}%d%M@%c"
msgminwait 1
msgwait 1
startup_message off
termcapinfo xterm ti@:te@
